# Copy this file to `.env` and fill in secrets for local development.
APP__OAUTH__CLIENT_ID="replace-with-google-oauth-client-id"
APP__OAUTH__CLIENT_SECRET="replace-with-google-oauth-client-secret"
APP__OAUTH__REDIRECT_URI="http://localhost:8080/oauth/callback"
APP__SECURITY__TOKEN_STORE_PATH="config/tokens.json"
APP__SECURITY__USE_IN_MEMORY="false"

# Optional (recommended for Cloud Run / remote deployments)
# APP__SERVER__PUBLIC_URL="https://your-service-url"
# APP__PROXY__ENABLED="true"
# APP__SERVER__BIND_ADDRESS="0.0.0.0:8080"
